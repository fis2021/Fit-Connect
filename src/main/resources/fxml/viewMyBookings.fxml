    <ListView fx:id="rezervarileMele" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="1"/>

    <Text fx:id="backMessage" GridPane.columnIndex="0" GridPane.rowIndex="7"/>
    <HBox alignment="bottom_left" spacing="10" GridPane.columnIndex="0" GridPane.rowIndex="5">
        <Button onAction="#handleBackSportsmanPageAction" text="Back"/>
